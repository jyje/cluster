                        "sampleLimit": {
                            "type": "boolean"
                        },
                        "targetLimit": {
                            "type": "boolean"
                        }
                    }
                },
                "podMonitor": {
                    "type": "object",
                    "properties": {
                        "enabled": {
                            "type": "boolean"
                        },
                        "port": {
                            "type": "string"
                        },
                        "extraLabels": {
                            "type": "object"
                        },
                        "annotations": {
                            "type": "object"
                        },
                        "interval": {
                            "type": "string"
                        },
                        "scrapeTimeout": {
                            "type": "string"
                        },
                        "relabelings": {
                            "type": "array"
                        },
                        "metricRelabelings": {
                            "type": "array"
                        },
                        "honorLabels": {
                            "type": "boolean"
                        },
                        "additionalLabels": {
                            "type": "object"
                        },
                        "podTargetLabels": {
                            "type": "array"
                        },
                        "sampleLimit": {
                            "type": "boolean"
                        },
                        "targetLimit": {
                            "type": "boolean"
                        }
                    }
                },
                "prometheusRule": {
                    "type": "object",
                    "properties": {
                        "enabled": {
                            "type": "boolean"
