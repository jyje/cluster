        "tolerations": {
            "type": "array"
        },
        "topologySpreadConstraints": {
            "type": "array",
            "items": {
                "type": "object"
            }
        },
        "valkeyConfig": {
            "type": "string"
        },
        "valkeyLogLevel": {
            "type": "string"
        }
