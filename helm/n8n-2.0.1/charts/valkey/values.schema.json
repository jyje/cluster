                "imagePullSecrets": {
                    "type": "array",
                    "default": [],
                    "items": {}
                }
            }
        },
        "affinity": {
            "type": "object"
        },
        "auth": {
            "type": "object",
            "properties": {
                "aclConfig": {
                    "type": "string"
                },
                "aclUsers" : {
                    "type": "object"
